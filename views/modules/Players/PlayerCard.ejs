<div class="team-player-card">
    <div class="team-player-card-left">
        <p class="player-number"><%- player.jerseyNumber %></p>
        <p class="player-name"><%- player.name %></p>
    </div>
    <div class="team-player-card-right">
        <a class="card-link" href="/playerdetail/<%- player.id %>">
            <img src="/assets/details.svg" alt="Details icon" class="card-btn-svg">
            Details
        </a>
    </div>
</div>